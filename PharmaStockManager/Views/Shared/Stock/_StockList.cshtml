@model IEnumerable<PharmaStockManager.Models.Stock>

<h2>Stock List</h2>
<table>
    <thead>
        <tr>
            <th>Symbol</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var stock in Model)
        {
            <tr>
                <td>@stock.Symbol</td>
                <td>@stock.Price.ToString("C")</td>
            </tr>
        }
    </tbody>
</table>